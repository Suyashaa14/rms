    },
    {
      "name": "Menu",
      "path": "/menu",
      "component": "MenuPage",
      "title": "Menu",
      "layout": "public",
      "showInNav": true
    },
    {
      "name": "Contact",
      "path": "/contact",
      "component": "ContactPage",
